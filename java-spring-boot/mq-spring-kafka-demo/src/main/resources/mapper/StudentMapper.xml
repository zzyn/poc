<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.mq.kafka.demo.mapper.StudentMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.mq.kafka.demo.entity.Student">
        <id column="custom_id" property="customId" />
        <result column="grade" property="grade" />
        <result column="created_by" property="createdBy" />
        <result column="created_timestamp" property="createdTimestamp" />
        <result column="updated_by" property="updatedBy" />
        <result column="updated_timestamp" property="updatedTimestamp" />
    </resultMap>

</mapper>
