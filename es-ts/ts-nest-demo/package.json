{
  "name": "ts-nest-demo",
  "version": "1.0.0",
  "description": "a web server",
  "author": "zack",
  "private": true,
  "scripts": {
    "v0:prebuild": "rimraf dist",
    "v0:build": "nest build",
    "v0:start": "nest start",
    "v0:start:dev": "nest start --watch",
    "v0:start:debug": "nest start --debug --watch",
    "v0:start:prod": "node dist/main",
    "v0:lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "v0:test": "jest",
    "v0:test:watch": "jest --watch",
    "v0:test:cov": "jest --coverage",
    "v0:test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "v0:test:e2e": "jest --config ./test/jest-e2e.json",
    "v0:format": "prettier --config '.prettierrc.js' --ignore-path '.gitignore' --write '**/*.{ts,tsx,js,jsx}'",
    "start": "nodemon --exec ts-node --files src/main.ts",
    "lint": "eslint --fix --ext '.js,.jsx,.ts,.tsx' .",
    "build": "tsc -d & cp -f public_key.pem build/ & cp -rf src/public build/",
    "package": "webpack --progress && cp -f public_key.pem dist/ && cp -f -r yaml dist/",
    "format": "prettier --config '.prettierrc.js' --ignore-path '.gitignore' --write '**/*.{ts,tsx,js,jsx}'",
    "test": "NODE_ENV=test jest -c jest.config.js"
  },
  "dependencies": {
    "@chankamlam/nest-jaeger": "^1.0.16",
    "@nestjs/common": "^7.5.1",
    "@nestjs/config": "^0.6.1",
    "@nestjs/core": "^7.5.1",
    "@nestjs/platform-express": "^7.5.1",
    "@nestjs/serve-static": "^2.1.4",
    "@nestjs/swagger": "^4.7.5",
    "@nestjs/terminus": "^7.0.1",
    "@nestjs/typeorm": "^7.1.5",
    "@types/uuid": "^8.3.0",
    "jaeger-client": "^3.18.1",
    "mysql": "^2.18.1",
    "ramda": "^0.27.1",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^6.6.3",
    "sqlite3": "^5.0.0",
    "swagger-ui-express": "^4.1.5",
    "typeorm": "^0.2.29",
    "typeorm-typedi-extensions": "^0.2.3"
  },
  "devDependencies": {
    "@nestjs/cli": "^7.5.1",
    "@nestjs/schematics": "^7.1.3",
    "@nestjs/testing": "^7.5.1",
    "@types/express": "^4.17.8",
    "@types/jaeger-client": "^3.15.4",
    "@types/jest": "^26.0.15",
    "@types/js-yaml": "^3.12.5",
    "@types/node": "^14.14.6",
    "@types/ramda": "^0.27.33",
    "@types/supertest": "^2.0.10",
    "@typescript-eslint/eslint-plugin": "^4.6.1",
    "@typescript-eslint/parser": "^4.6.1",
    "eslint": "^7.12.1",
    "eslint-config-airbnb-typescript": "^6.3.1",
    "eslint-config-prettier": "7.0.0",
    "eslint-config-standard-with-typescript": "^19.0.1",
    "eslint-plugin-eslint-comments": "^3.1.2",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-jest": "^23.8.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.19.0",
    "eslint-plugin-unicorn": "^17.1.0",
    "jest": "^26.6.3",
    "nodemon": "^2.0.6",
    "prettier": "^2.1.2",
    "supertest": "^6.0.0",
    "ts-jest": "^26.4.3",
    "ts-loader": "^8.0.8",
    "ts-node": "^9.0.0",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^4.1.2",
    "webpack": "^4.42.1",
    "webpack-cli": "^3.3.11"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
